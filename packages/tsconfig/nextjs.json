{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Next.js",
  "extends": "./base.json",
  "compilerOptions": {
    "plugins": [{ "name": "next" }],
    "paths": {
      "@/*": ["./*"],
      "@ui/*": ["../../packages/ui/*"],
      "@/ui/components/*": ["../../packages/ui/components/ui/*"],
      "@/ui/components/ui/*": ["../../packages/ui/components/ui/*"]
    }
  },
  "include": ["src", "next-env.d.ts"],
  "exclude": ["node_modules"]
}
