{
  "extends": "tsconfig/nextjs.json",
  "compilerOptions": {
    "plugins": [{ "name": "next" }],
		"baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
			"ui/*", ["../../packages/ui/*"],
			"ui/components/ui/*", ["../../packages/ui/components/ui/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
  "exclude": ["node_modules"]
}
